{
    "deno.enable": true,
    "editor.formatOnSave": true,
    "files.exclude": {
        ".github": true,
        ".vscode": true,
        "*-journal": true,
        "*.br": true,
        "**/.gitignore": true,
        "**/*.bak": true,
        "**/*.ignore": true,
        "**/*.lock": true,
        "**/*.sqlite": true,
        "**/lock.*": true,
        "canvaskit.wasm": true
    },
    "remote.WSL.fileWatcher.polling": true,
    "debug.javascript.unmapMissingSources": true,
    "deno.unstable": true,
}
